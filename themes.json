    ],
    "module": {
      "name": "shiraha",
      "id": "denoland/lume_theme_shiraha",
      "origin": "https://deno.land/x/lume_theme_shiraha",
      "main": "/mod.ts",
      "cms": "/_cms.ts",
      "srcdir": "/src",
      "src": [
        "/articles/getting-started.md",
        "/articles/part-1-installation.md",
        "/articles/part-1-writing.md"
      ],
      "compilerOptions": {
        "jsx": "precompile",
        "jsxImportSource": "shiraha",
        "jsxFactory": "jsx",
        "jsxFragmentFactory": "Fragment"
      },
      "imports": {
        "shiraha/jsx-runtime": "./jsx-runtime.ts"
      },
      "unstable": ["ffi"]
    }
  },
